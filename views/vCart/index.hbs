<div class="card">
  <div class="card-header">Category</div>
  <div class="card-body">
    {{#if this.isEmpty}}
    <h4 class="card-title">No data</h4>
    <p class="card-text">Text</p>
    {{else}}
    <table class="table table-hover">
      <thead>
        <tr>
          <th scope="col">Product</th>
          <th scope="col">Price</th>
          <th scope="col">Quantity</th>
          <th scope="col">Amount</th>
          <th scope="col">&nbsp;</th>
        </tr>
      </thead>
      <tbody>
        {{#each this.items}}
        <tr>
          <th scope="row">
            <a href="/products/detail/{{id}}">
              {{product.ProName}}
            </a>
          </th>
          <td>{{product.Price}}</td>
          <td>{{quantity}}</td>
          <td>{{format Amount}}</td>
          <td class="text-left">
            <a name="" id="" class="btn-sm btn-success" href="javascript:;" title="Remove" role="button">
              <i class="fa fa-eraser" aria-hidden="true"></i>
            </a>
          </td>
        </tr>
        {{/each}}
      </tbody>
      <tfoot>
        <th>
          <b> Total: </b>
        </th>
        <td>&nbsp;</td>
        <td>&nbsp;</td>
        <td>
          <span class="text-danger font-weight-bold">
            {{format totalMoney}}
          </span>
        </td>
        <td>&nbsp;</td>
      </tfoot>
    </table>
    {{/if}}
  </div>
  <div class="card-footer">


  </div>
</div>;